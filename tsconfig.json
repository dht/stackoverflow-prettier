{
  "compilerOptions": {
    "target": "es2016",
    "module": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noLib": true,
    "noResolve": true,
    "declaration": false,
    "noEmitOnError": false,
    "composite": false
  }
}
